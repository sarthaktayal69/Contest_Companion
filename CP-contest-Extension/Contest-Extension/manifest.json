{
    "name": "Contest Calendar Extension",
    "description": "Chrome extension to keep you updated on the upcoming CP contest of all major platforms",
    "permissions": [
        "activeTab", 
        "declarativeContent", 
        "storage"
    ], 
    "version": "1.0.0",
    "icons": {
    "128": "logo/128.png",
    "96": "logo/96.png",
    "48": "logo/48.png",
    "16": "logo/16.png"
    }, 
    "page_action": {
    "default_popup": "index.html",
    "default_icon": "images/logo-default.png"
    },
    "background": {
    "scripts": ["js/background.js"],
    "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2

}